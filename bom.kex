Posit: Procedure
String = Arg(1)
If Pos('^POS^',String) \= 0
   Then Do
        Words = Translate(String,' ','^')
        Pos = ''
        MaxCol = 0
        Fields = ''
        Do I = 1 To Words(Words)
           Word = Word(Words,I)
           If Word \= 'POS' & Word \= 'COL' & Substr(Word,1,1) \= "+" & Substr(Word,1,1) \= "-"
              Then Do
                   If Pos = ''
                      Then Pos = '\\\^999'
                   Fields = Fields' 'Pos'^'Word
                   Pos = ''
                   End
              Else Do
                   If Substr(Word,1,1) = "+" | Substr(Word,1,1) = "-"
                      Then Do
                           Word = Substr(Word,1,1)''LPad(Substr(Word,2),2)
                           Next = '000'
                           End
                      Else Do
                           I = I + 1
                           Next = Word(Words,I)
                           If DataType(Next) = 'NUM'
                              Then Next = LPad(Next,3)
                           End
                   If Word = 'COL'
                      Then Word = 'col'
                   If Word = 'POS'
                      Then Do
                           If Next > MaxCol & DataType(Next) = 'NUM'
                              Then MaxCol = Next
                           End
                   Pos = Word'^'Next
                   End
        End
        'Kedit 'Pgm'_'Report'.pos (NoMsg'
        'Del *'
        'Add 'MaxCol
        Do I = 1 To Words(Fields)
           Field = Translate(Word(Fields,I),' ','^')
           P1 = Word(Field,1)
           P2 = Word(Field,2)
           P3 = Word(Field,3)
           If P1 = 'POS'
              Then ':'P2
              ELse Do
                   If P1 \= '\\\'
                      Then 'Cappend  'P1' '
                   End
           'Extract/CurLine'
           'CAppend  'P3
        End
        'All Blank'
        If Rc = 0 Then 'Del *'
        'All'
        String = ''
        'Top'
        Do Forever
           'Next'
           If Rc \= 0 Then Leave
           'Extract/CurLine'
           String = String' 'Space(CurLine.3)
           * BUG ??? delimitador
        End
        String = Translate(Space(String),'^',' ')
        'QQuit'
        End
Return String

